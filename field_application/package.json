{
    "name": "field_application",
    "description": "Field application for the Flower Power project",
    "author": "Vincent Venhuizen & Sueno de kort",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "electron:build": "vue-cli-service electron:build",
        "electron:serve": "vue-cli-service electron:serve",
        "python:build": "pyinstaller api.spec --noconfirm --distpath public/backend_dist",
        "python_py:build": "pyinstaller public/backend/api.py --noconfirm --distpath public/backend_dist",
        "flowerpower:build": "pyinstaller api.spec --noconfirm --distpath public/backend_dist && vue-cli-service electron:build",
        "postinstall": "electron-builder install-app-deps",
        "postuninstall": "electron-builder install-app-deps",
        "rebuild": "electron-rebuild -f -w json-rpc2"
    },
    "main": "background.js",
    "dependencies": {
        "@mdi/font": "^3.9.97",
        "@mdi/js": "^5.8.55",
        "axios": "^0.21.0",
        "core-js": "^3.6.5",
        "d": "^1.0.1",
        "electron-dl": "^3.0.2",
        "moment": "^2.29.1",
        "nedb": "^1.8.0",
        "nedb-promises": "^4.1.0",
        "roboto-fontface": "*",
        "vue": "^2.6.11",
        "vue-router": "^3.2.0",
        "vue2-google-maps": "^0.10.7",
        "vuetify": "^2.2.11",
        "vuex": "^3.4.0"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.5.0",
        "@vue/cli-plugin-eslint": "^4.5.9",
        "@vue/cli-plugin-router": "~4.5.0",
        "@vue/cli-plugin-vuex": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "babel-eslint": "^10.1.0",
        "electron": "^9.0.0",
        "electron-devtools-installer": "^3.1.0",
        "eslint": "^6.7.2",
        "eslint-plugin-vue": "^6.2.2",
        "sass": "^1.19.0",
        "sass-loader": "^8.0.0",
        "vue-cli-plugin-electron-builder": "~2.0.0-rc.5",
        "vue-cli-plugin-vuetify": "~2.0.7",
        "vue-template-compiler": "^2.6.11",
        "vuetify-loader": "^1.3.0"
    }
}
