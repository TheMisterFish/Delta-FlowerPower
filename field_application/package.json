{
  "name": "flowerpower-field-application",
  "version": "1.0.0",
  "private": true,
  "description": "A Electron and Python FlowerPower field application",
  "author": {
    "name": "fyears"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "electron_build": "electron-packager . --overwrite --ignore='backend$' --ignore='\\.venv' --out=release-builds",
    "make_build": "npm run install && npm run python_build && npm run electron_build",
    "python_build": "pyinstaller backend/api.py --noconfirm --distpath backenddist",
    "rebuild_electron": "electron-rebuild",
    "start": "electron ."
  },
  "main": "main.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "electron-packager": "^15.1.0",
